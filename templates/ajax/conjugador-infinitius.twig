<section  class="formulari-eines contingut-section">
   
         <!-- .conjugador-resultat  -->
                        <div class="diccionari-resultat">

{% if response %}
    <h1>Possibles infitius</h1>
    <p>Indica l'infinitiu que vols conjugar:</p>
    {%for row in response.verbs|batch((response.verbs|length)/2) %}
        <div class="col-xs-6 col-md-6">
            <ul class="multilingue_lletres">
            {% for word in row %}

                <li><a href="/conjugador-de-verbs/verb/{{ word | keys | first }}/">{{ word | keys | first }}</a></li>
            {% endfor %}
            </ul>
        </div>
    {% endfor %}
{% endif %}


</div><!--/.conjugador-resultat  -->

        </section><!--/.formulari-eines  -->